{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ],
      "run_at": "document_start"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "tabreorder.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "browser_action": {
      "default_icon": "ruby-128.png",
      "default_popup": "ruby-440.png",
      "default_title": "Tab Mover"
   },
   "manifest_version": 2,
   "name": "Mac Hotkey for Tab Moving",
   "description": "Reorder tabs with cmd+shift+p and cmd+shift+\\.",
   "permissions": [ ],
   "version": "0.0.2"
}
