{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ],
      "run_at": "document_start"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "tabreorder.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "browser_action": {
      "default_icon": "ruby.jpg",
      "default_popup": "ruby.jpg",
      "default_title": "tab mover"
   },
   "manifest_version": 2,
   "name": "Hotkey for Tab Moving",
   "description": "Reorder tabs with cmd+shift+p and cmd+shift+\\.",
   "permissions": [ ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.1"
}
