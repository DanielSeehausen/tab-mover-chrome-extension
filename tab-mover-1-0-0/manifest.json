{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "commands": {
      "move-tab-left": {
        "suggested_key": {
          "default": "Ctrl+Shift+O",
          "mac": "Command+Shift+O"
        },
        "description": "Moves Current Tab One to the Left"
      },
      "move-tab-right": {
        "suggested_key": {
          "default": "Ctrl+Shift+P",
          "mac": "Command+Shift+P"
        },
        "description": "Moves Current Tab One to the Right"
      }
    },
   "browser_action": {
      "default_icon": "ruby-128.png",
      "default_popup": "ruby-440.png",
      "default_title": "Tab Mover"
   },
   "manifest_version": 2,
   "name": "Mac Hotkey for Tab Moving",
   "description": "Reorder tabs with cmd+shift+p and cmd+shift+\\.",
   "permissions": [ ],
   "version": "0.1.0"
}
